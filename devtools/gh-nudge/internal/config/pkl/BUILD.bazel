load("@rules_go//go:def.bzl", "go_library")

# Go library with pre-generated code from pkl-gen-go
# To regenerate: cd devtools/gh-nudge/pkl && pkl-gen-go Config.pkl --output-path ../internal/config/pkl/
go_library(
    name = "go_default_library",
    srcs = glob(["*.go"]),
    importpath = "github.com/jaeyeom/experimental/devtools/gh-nudge/internal/config/pkl",
    visibility = ["//devtools/gh-nudge:__subpackages__"],
    deps = [
        "@com_github_apple_pkl_go//pkl",
    ],
)
